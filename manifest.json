{
	"manifest_version" : 2,

	"name" : "SF IMDB Rating", 
	"description" : "Displays the IMDB ratings on sf.se movie posters",
	"version":  "1.0000000",

	"web_accessible_resources" : ["img/*"],
	"background": {
		"scripts":["js/bg.js"]
	},

	 "content_scripts": [
    {
      "matches": ["http://www.sf.se/*"],
      "css": ["css/style.css"],
      "js": ["js/jquery.js", "js/main.js"]
    }
  ],

	"browser_action": {
		"default_icon" : {"48": "img/icon48.png"}
	},

	"permissions" : [
		"activeTab",
		"tabs", "http://www.sf.se/*"
	]
}